<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
			"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
			"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="mx.habil.capacitacion.dto.CarreraDto" table="CARRERA">

		<id name="idCarrera" column="ID_CARRERA">
			<generator class="identity" />
		</id>
		<property name="nombre" type="string" column="NOMBRE_CARRERA" />
		<property name="edificio" type="string" column="EDIFICIO" />
		<property name="jefeCarrera" type="string" column="JEFE_CARRERA" />
		<property name="laboratorio" type="string" column="LABORATORIO" />
	</class>


</hibernate-mapping>
